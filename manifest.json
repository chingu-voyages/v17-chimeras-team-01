{
  "name": "My tabs",
  "version": "1.0",
  "description": "This extension helps you reduce tab clutter!",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "My tabs",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": ["storage", "tabs", "contextMenus", "chrome://favicon/"]
}
